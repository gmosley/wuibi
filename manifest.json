{
  "manifest_version": 2,
  "name": "Wuibi",
  "version": "0.0.1",
  "description": "Unofficial Quibi Web Player",
  "permissions": [
    "cookies",
    "webRequest",
    "webRequestBlocking",
    "*://*/*"
  ],
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "browser_action": {},
  "web_accessible_resources": [
    "dist/wuibi.html",
    "dist/main.js",
    "dist/shaka-player.ui.js",
    "dist/shaka-player.ui.debug.js",
    "dist/controls.css",
    "dist/browser-polyfill.js"
  ]
}